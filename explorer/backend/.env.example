# XAI Explorer Backend Configuration

# Database
DATABASE_URL=postgresql://xai:xai@localhost:5432/xai_explorer

# XAI Node
XAI_NODE_URL=http://localhost:12001
XAI_NODE_API_KEY=

# Explorer API auth
EXPLORER_API_KEY_SECRET_PATH=~/.xai/explorer_api_keys.json
EXPLORER_REQUIRE_API_KEY=true

# Server
HOST=0.0.0.0
PORT=12080
RELOAD=false

# CORS
CORS_ORIGINS=http://localhost:12080,http://localhost:5173

# Redis (optional)
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=INFO
