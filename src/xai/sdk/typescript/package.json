{
  "name": "@xai/sdk",
  "version": "1.0.0",
  "description": "Production-ready TypeScript/JavaScript SDK for the XAI blockchain",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "xai",
    "blockchain",
    "sdk",
    "cryptocurrency",
    "web3",
    "ethereum",
    "typescript"
  ],
  "author": "XAI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xai-blockchain/xai.git",
    "directory": "sdk/typescript"
  },
  "bugs": {
    "url": "https://github.com/xai-blockchain/xai/issues"
  },
  "homepage": "https://xai-blockchain.io",
  "dependencies": {
    "axios": "^1.6.2",
    "axios-retry": "^4.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
