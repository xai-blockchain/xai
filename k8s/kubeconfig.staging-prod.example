apiVersion: v1
kind: Config
preferences: {}
clusters:
  - name: xai-staging
    cluster:
      server: https://k8s-staging.xai.invalid:6443
      certificate-authority-data: REPLACE_BASE64_ENCODED_STAGING_CA_PEM
  - name: xai-prod
    cluster:
      server: https://k8s-prod.xai.invalid:6443
      certificate-authority-data: REPLACE_BASE64_ENCODED_PROD_CA_PEM
users:
  - name: xai-staging-admin
    user:
      client-certificate-data: REPLACE_BASE64_ENCODED_STAGING_CLIENT_CERT_PEM
      client-key-data: REPLACE_BASE64_ENCODED_STAGING_CLIENT_KEY_PEM
  - name: xai-prod-admin
    user:
      client-certificate-data: REPLACE_BASE64_ENCODED_PROD_CLIENT_CERT_PEM
      client-key-data: REPLACE_BASE64_ENCODED_PROD_CLIENT_KEY_PEM
contexts:
  - name: xai-staging
    context:
      cluster: xai-staging
      user: xai-staging-admin
      namespace: xai-blockchain
  - name: xai-staging-monitoring
    context:
      cluster: xai-staging
      user: xai-staging-admin
      namespace: xai-blockchain-monitoring
  - name: xai-prod
    context:
      cluster: xai-prod
      user: xai-prod-admin
      namespace: xai-blockchain
  - name: xai-prod-monitoring
    context:
      cluster: xai-prod
      user: xai-prod-admin
      namespace: xai-blockchain-monitoring
current-context: ""
