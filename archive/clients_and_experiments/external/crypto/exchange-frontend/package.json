{
  "name": "aixn-exchange-frontend",
  "version": "1.0.0",
  "description": "AIXN P2P Exchange Frontend - Secure Trading Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js --fix",
    "format": "prettier --write \"**/*.{js,json,html,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "snyk test",
    "clean": "rimraf dist"
